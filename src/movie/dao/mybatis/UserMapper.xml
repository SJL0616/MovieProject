<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="movie.dao.mybatis.UserMapper">

	<select id="getTheUserById" parameterType="String"
		resultType="user">
		select * from user where id=#{id}
	</select>

</mapper>
